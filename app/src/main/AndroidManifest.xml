<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jbbmobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".view.RegisterScreenActivity"/>
        <activity android:name=".view.ExampleActivity" />
        <activity android:name=".view.LoginActivity">
        </activity>
        <activity android:name=".view.StartScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".view.MainScreenActivity">
        </activity>
        <activity android:name=".view.PreferenceScreenActivity"></activity>
    </application>

</manifest>